                    $('.nav_eject_Box').hide();
                })
                /*导航弹出层 结束*/

                $('.nav_eject_Bul li a').click(function(){
                    $('.nav_eject_Box').hide();
                })

                $(".ft_teshuxu").hover(function(){
                    $(".ft_ying_weixin").show();
                },function(){
                    $(".ft_ying_weixin").hide();
                })

            })

