    <script src="http://www.bmy.com.cn/bmy-websitems-1.0-SNAPSHOT/static/site/lib/jquery-1.7.2.min.js"></script>
    <script src="http://www.bmy.com.cn/bmy-websitems-1.0-SNAPSHOT/static/site/common/js/common.js"></script>
    <script src="http://www.bmy.com.cn/bmy-websitems-1.0-SNAPSHOT/static/site/lib/template-web.js"></script>
    <script src="http://www.bmy.com.cn/bmy-websitems-1.0-SNAPSHOT/static/site/lib/paginationjs-master/dist/pagination.min.js"></script>
    <script src="http://www.bmy.com.cn/bmy-websitems-1.0-SNAPSHOT/static/site/lib/busy-load-master/dist/app.min.js"></script>
    <script>
      $(function () {
         globSearch('http://'+window.location.host+'/bmy-websitems-1.0-SNAPSHOT/static/site/public/pages/searchList.html', 'ads122s1a3d12s1da1w4s1qr41s4gg1j')
        var obj = parseQueryString()

        if (obj.mode ==='教育活动') {
          $('.titleHonor').attr('src', 'http://www.bmy.com.cn/bmy-websitems-1.0-SNAPSHOT/static/site/public/img/news-more/eduTitle.png')
        } else if(obj.mode ==='资讯') {
          $('.titleHonor').attr('src', 'http://www.bmy.com.cn/bmy-websitems-1.0-SNAPSHOT/static/site/public/img/news-more/newsTitle.png')
        }

        // 发送ajax 传递的参数
        var params = {
          currentPage: 1,
          showCount: 15,
          code: obj.code
        }


        // 加载页面时首次请求数据
        getData(params, function (data) {
          
          // 模板引擎渲染
          $('.itemBox').html(template('list', data))

          randePage(data)
        })

        // ajax 获取数据
        function getData(params, callback) {
          $('.centerBox').busyLoad("show", {
            background: "rgba(0, 0, 0, 0.21)",
            spinner: "circles",
            animation: "slide"

          })
          $.ajax({
            type: "get",
            url: "/bmy-websitems-1.0-SNAPSHOT/bmy/list.do",
            data: params,
            dataType: "json",
            success: function (data) {

              for (var i = 0; i < data.list.length; i++) {
                data.list[i].publishtime = fmtDate(data.list[i].publishtime)
              }
              $('.centerBox').busyLoad("hide")
              callback(data)
            }
          });
        }

        var flag = false
        function randePage(data) {
          $('.wrapper').pagination({
            // 渲染分页的页码
            dataSource: function () {
              var result = [];
              for (var i = 1; i < data.page.totalPage * data.page.showCount; i++) {
                result.push(i);
              }
              return result;
            }(),
            // 每页的数据条数15
            pageSize: data.page.showCount,
            activeClassName: 'act',
            nextText: '',
            prevText: '',
            callback: function (data, pagination) {

              params.currentPage = pagination.pageNumber
              if (flag) {
              getData(params, function (data) {
                // 避免放进入页面首次调用数据是重复渲染数据
                  $('.itemBox').html(template('list', data))
                })
              }
              flag = true


            }
          })
        }
      })
    </script>


