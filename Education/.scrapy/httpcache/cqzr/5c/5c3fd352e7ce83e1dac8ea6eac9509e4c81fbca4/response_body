<script type="text/javascript">
    function checkKey(style){
        var txtkeyWord = document.getElementById("txtkeyWord");
        txtkeys = txtkeyWord.value;
        txtkeys = txtkeys.replace(/(^\s*)|(\s*$)/g,'');
        switch(style){
            case 1:
                if (txtkeys == "输入关键字") {
                    txtkeyWord.value = "";
                }
                break;
            case 2:
                if (txtkeys == "") {
                    txtkeyWord.value = "输入关键字";
                }
                break;
        }
    }
    function goSearch() {
        var txtkeyWord = document.getElementById("txtkeyWord");
        txtkeys = txtkeyWord.value;
        txtkeys = txtkeys.replace(/(^\s*)|(\s*$)/g,'');
        if (txtkeys == "" || txtkeys == "输入关键字") {
            alert("请填写搜素关键词");
            return true;
        }
        self.location = "/search.asp?keys=" + escape(txtkeyWord.value) + "&id=" + 1;
        return false;
    }
</script>
