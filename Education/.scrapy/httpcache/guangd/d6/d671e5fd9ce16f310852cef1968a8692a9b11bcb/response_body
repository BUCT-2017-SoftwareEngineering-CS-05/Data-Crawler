<script>
$(function(){
  setInterval(go,100)
  var num=0
  
  function go(){
     $('body div').first().addClass('addmoban');
     
  }
  
 //导航图片





   $('#head_nav_img').load('/gdmuseum/dhtp/index.html .head_nav_img')
  setTimeout(nav,300)

  function nav(){
    $(' .top_nav .nav_ul .nli').each(function(){
       var i=$(this).index();
       
       var nav_img=$('#head_nav_img .head_nav_img dd').eq(i).html();
       var c_len= $(this).find('.c_img img').length;
       $(' .top_nav .nav_ul .nli').eq(i+1).find('.c_img').empty().append(nav_img)

    })
  }

//非法
 $('.jianjie_head #title.title').each(function(){
     $(this).removeClass('title').addClass('title1')

 })
   

 //关闭登录弹出
 $('.soon-close').click(function(){
    $('.dl_ifram').hide();
 })



})
 function tc_show(){
    $('.dl_ifram').show();
 }
