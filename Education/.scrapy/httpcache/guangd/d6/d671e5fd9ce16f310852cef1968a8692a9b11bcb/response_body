<script>
 $(function(){
    $('#list .list_ul img').each(function(){
        var str=$(this).attr('src');
        if(str.indexOf('/fileDir')==0){
           str=str.substr(8)
           $(this).attr('src',str);
        }
    })
    $('#detail1 .banner li img').each(function(){
      var src= $(this).attr('src');

      if(src.indexOf('attachment')==0){

         src='/'+src;
         $(this).attr('src',src)
        
      }
    })
    $('#content .product img').each(function(){
        var str=$(this).attr('src');
        if(str.indexOf('/fileDir')==0){
           str=str.substr(8)
           $(this).attr('src',str);
        }
    })
    $('img').each(function(){
        var str=$(this).attr('src');
        str=str.replace(/\s+/,'')

        if(str=='/'){
          $(this).hide();
          $(this).parent('.txt_img').hide();
          $(this).closest('li').find('.txt_cont').css('width',880);
          $(this).remove();
        }else if(str==''){
          $(this).hide()
          $(this).parent('.txt_img').hide();
          $(this).closest('li').find('.txt_cont').css('width',880);
          $(this).remove();
        }
    })
   


  //img兼容
  $('img').each(function(){
      var src= $(this).attr('src');
      try{
        if(src.indexOf('/fileDir')==0){
           src='/eportal'+src;
           $(this).attr('src',src);
         }else if(src.indexOf('fileDir')==0){
             src='/eportal/'+src;
             $(this).attr('src',src);
         }
       }catch(err){}



 
     var a=src;
     a.replace(/\s+/,'')
     if(src=='/'){
         $(this).hide();
     }
  });
  setInterval(function(){
       $('img').each(function(){

             var href=$(this).attr('src');

             if(href.indexOf('attachment')==0){
                 href='/'+href
                 $(this).attr('src',href);
             }
       })
  },300)

})


</script>
<script>
$(function(){
   $('.foot_nav .foot_wrap dl dd').each(function(){
      var ind = $(this).index();
      var p=$('.child_en span').eq(ind).text();
      var pw=$('.child_en span').eq(ind).width();
      $(this).append('<p>'+p+'</p>')
      $(this).find('p').css({'width':pw,'margin-left':-pw/2})
      
   })
   $('.child_en').css('display','none')

   //页码
   var font=$('.NormalRed').text();
   $('.NormalRed').after('<a class="cur_page" style="color:red;border:1px solid #A55656;">'+font+'</a>')
   $('.NormalRed').hide();



   //子站顶部链接
   $('#header_logo a').attr('href','/gdmuseum/index.html')

    //来源
   var link=$('.info .link').text()
   
   try{var link1=link.replace(/\s+/,'') }catch(e){var link1=''}
   try{var link2=link.index('广东') }catch(e){var link2=-1}
   try{var link3=link.index('博物') }catch(e){var link3=-1}
   if(link1==''||link2>0&&link3>0){
       $('.info .link').text('广东省博物馆');
   }
   //底部二维码
  $('.foot_ewm').load('/gdmuseum/zbgddh/dbwxewm/index.html #db_ewm')
  


})



/*function uaredirect(f) {
    try {
        if (document.getElementById("bdmark") != null) {
            return
        }
        var b = false;
        if (arguments[1]) {
            var e = window.location.host;
            var a = window.location.href;
            if (isSubdomain(arguments[1], e) == 1) {
                f = f + "/#m/" + a;
                b = true
            } else {
                if (isSubdomain(arguments[1], e) == 2) {
                    f = f + "/#m/" + a;
                    b = true
                } else {
                    f = a;
                    b = false
                }
            }
        } else {
            b = true
        }
        if (b) {
            var c = window.location.hash;
            if (!c.match("fromapp")) {
                if ((navigator.userAgent.match(/(iPhone|iPod|Android|ios)/i))) {
                    location.replace(f)
                }
            }
        }
    } catch(d) {}
}
function isSubdomain(c, d) {
    this.getdomain = function(f) {
        var e = f.indexOf("://");
        if (e > 0) {
            var h = f.substr(e + 3)
        } else {
            var h = f
        }
        var g = /^www\./;
        if (g.test(h)) {
            h = h.substr(4)
        }
        return h
    };
    if (c == d) {
        return 1
    } else {
        var c = this.getdomain(c);
        var b = this.getdomain(d);
        if (c == b) {
            return 1
        } else {
            c = c.replace(".", "\\.");
            var a = new RegExp("\\." + c + "$");
            if (b.match(a)) {
                return 2
            } else {
                return 0
            }
        }
    }
};


        function getUrlParam(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
            var r = window.location.search.substr(1).match(reg);  //匹配目标参数
            if (r != null) return unescape(r[2]); return null; //返回参数值
        }
      var tz = getUrlParam("tz");
      var loc_a=window.location.href;
      var pd=loc_a.indexOf('tz=')
	if(pd>0){
          $('a').each(function(){
               var a_h=$(this).attr('href')
              try{ var a_h2=a_h.replace(/\s+/);}catch(err){};
               if(a_h2!=''){
                try{
                  if(a_h.indexOf('?')>0){
                     a_h=a_h+'&tz=1';
                     $(this).attr('href',a_h);
                  }else{
                     a_h=a_h+'?tz=1';
                     $(this).attr('href',a_h);
                  }
                }catch(err){}
             }

          })
        }else{
		uaredirect("http://www.gdmuseum.com/webphone/");
	}*/
</script>
