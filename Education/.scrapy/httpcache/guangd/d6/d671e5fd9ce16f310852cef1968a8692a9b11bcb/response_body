$(function(){
   $('img').each(function(){
      var src= $(this).attr('src');
       try{
        if(src.indexOf('/eportal/http')==0){
           src='http'+src.split('http')[1];
           $(this).attr('src',src);
         }
       }catch(err){}
   })
})

var today = new Date();
$(function(){
    $('.js_img').each(function(){
        // console.log($(this).find(".act_start").text());
        if($(this).find(".act_start").text()==null||$(this).find(".act_start").text()==''){
            $(this).find('.kfyuyue').addClass('no_yy').css('display','block');
        }
        else if($(this).find(".act_start").text()!=null&&$(this).find(".act_start").text()!=" "){
            var startTime=$(this).find('.act_start').text();
            var endTime=$(this).find('.act_end').text();
            var start_time=new Date(startTime.replace("-", "/").replace("-", "/"))
            var end_time=new Date(endTime.replace("-", "/").replace("-", "/"))
            // console.log(start_time);
            if(today<start_time){
               $(this).find('.kfyuyue').addClass('red').css('display','block');
            }else if (today>start_time&&today<end_time)
            {
               $(this).find('.kfyuyue').addClass('green').css('display','block');
            }else if(today>end_time){
               $(this).find('.kfyuyue').addClass('hui').css('display','block');
            }
        }
    })
})
