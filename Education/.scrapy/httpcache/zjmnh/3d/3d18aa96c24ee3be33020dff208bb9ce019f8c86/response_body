<script>var app = new Vue({
    el: '#app',
    mounted: function () {
        var swiper = new Swiper('.swiper-container', {
            direction: 'vertical',
            slidesPerView: 'auto',
            freeMode: true,
            scrollbar: {el: '.swiper-scrollbar',},
            mousewheel: true,
        });
        var dictype = window.application.getQuery('dictype');
        this.dicType = dictype;
        this.getCat(dictype);
        this.getClassicId(dictype);
    },
    methods: {
        onArrowClick: function (e) {
            if (e == 1) {
                document.querySelector('.imagelist').scrollBy({behavior: 'smooth', left: -226, top: 0});
            } else {
                document.querySelector('.imagelist').scrollBy({behavior: 'smooth', left: 226, top: 0});
            }
        }, getClassicId: function (res) {
            $.get(window.application.serverPath.findDicByTypeId, {typeId: 13}, function (res) {
                var i = 0;
                for (var key in res) {
                    this.navdatas[i].dicType = key;
                    this.navdatas[i].title = res[key];
                    i++;
                }
            }.bind(this));
        }, getTitleWithType: function (type) {
            for (var i = 0; i < this.navdatas.length; i++) {
                if (type == this.navdatas[i].dicType) {
                    return this.navdatas[i].title;
                }
            }
        }, onSubNavClick: function (type, data, index) {
            this.selectcat(index);
            this.getInfo(type);
        }, selectcat: function (index) {
            for (var i = 0; i < this.subNavs.length; i++) {
                if (index == i) {
                    this.subNavs[i].colorclass = 'defselectcolor com-flex-display com-flex-items-center';
                } else {
                    this.subNavs[i].colorclass = 'selectcolor com-flex-display com-flex-items-center';
                }
            }
            this.navarray[2].name = '详情';
        }, getInfo: function (titleKey) {
            $.get(window.application.serverPath.getInfo, {
                dicTypeId: this.dicType,
                titleKey: titleKey,
                category: 36,
                id: titleKey
            }, function (res) {
                this.infoData = res;
                $.get(window.application.serverPath.getActivitylist, {activityClzId: titleKey}, function (res) {
                    var images = [];
                    for (var i = 0; i < res.data.length; i++) {
                        images.push({
                            url: res.data[i].img,
                            id: res.data[i].id,
                            title: res.data[i].title,
                            createTime: res.data[i].createTime
                        })
                    }
                    this.huodongimages = images;
                }.bind(this));
                this.currentid = res.id;
                this.getNewsList(res.id);
            }.bind(this));
        }, onPageClicked: function (data) {
            this.getNewsList(this.currentid, data.page);
        }, getNewsList: function (id, page) {
            $.get(window.application.serverPath.findRelationNews, {
                entityId: id,
                category: 21,
                limit: 5,
                page: page
            }, function (resp) {
                this.newsList = resp.data;
                for (var i = 0; i < this.newsList.length; i++) {
                    this.newsList[i].href = window.application.createURL('/news/news_info.html', {
                        category: 1,
                        infotype: 8,
                        id: resp.data[i].id
                    });
                }
                this.totalcount = resp.count;
                this.totalpage = resp.totalPageNum;
                // console.log(this.newsList);
            }.bind(this))
        }, getCat: function (d) {
            var self = this;
            var type = window.application.getQuery('dictype');
            if (type > 8) {
                type = 1;
            }
            $.get(window.application.serverPath.findJiaoyuxiangqingCat, {dicType: 1}, function (resp) {
                console.log(JSON.stringify(resp));
            })
            $.get(window.application.serverPath.findJiaoyuxiangqingCat, {dicType: type,}, function (res) {
                self.subNavs.length = 0;
                var subtype = window.application.getQuery('subtype');
                var subtypeindex = 0;
                for (var i = 0; i < res.length; i++) {
                    self.subNavs.push({
                        title: res[i].title,
                        colorclass: 'selectcolor com-flex-display com-flex-items-center',
                        id: res[i].id,
                        select: false
                    })
                    if (res[i].id == subtype) {
                        subtypeindex = i;
                    }
                }
                if (subtype) {
                    self.selectcat(subtypeindex);
                    if (self.subNavs.length > 0) {
                        self.getInfo(self.subNavs[subtypeindex].id);
                    }
                } else {
                    self.selectcat(0);
                    if (self.subNavs.length > 0) {
                        self.getInfo(self.subNavs[0].id);
                    }
                }
            });
        }
    },
    data: {
        bgimg: '/images/jiaoyu/konglong.png',
        totalcount: 0,
        currentid: '',
        totalpage: 1,
        newsicon: '/images/imagenew/new_icon.png',
        newsList: [],
        navarray: [{name: '首页'}, {name: '教育'}, {name: '科普活动'}],
        infoData: {},
        dicType: '',
        nav_head_second_arrays: window.application.publicUIData.menuarray,
        huodongimages: [],
        subNavs: [],
        navdatas: [
            {
                dicType: 15,
                img: '1_1.png',
                title: '节日活动',
                href: ''
            },
            {
                dicType: 16,
                img: '2_1.png',
                title: '科普讲座',
                href: ''
            },
            {
                dicType: 17,
                img: '3_1.png',
                title: '自然课程',
                href: ''
            },
            {
                dicType: 18,
                img: '4_1.png',
                title: '自然艺术',
                href: ''
            },
            {
                dicType: 20,
                img: '5_1.png',
                title: '影视益智',
                href: ''
            },
            {
                dicType: 19,
                img: '6_1.png',
                title: '亲子教育',
                href: ''
            },
            {
                dicType: 21,
                img: '7_1.png',
                title: '野外体验',
                href: ''
            },
            {
                dicType: 22,
                img: '8_1.png',
                title: '综合实践',
                href: ''
            },]
    }
});</script>


