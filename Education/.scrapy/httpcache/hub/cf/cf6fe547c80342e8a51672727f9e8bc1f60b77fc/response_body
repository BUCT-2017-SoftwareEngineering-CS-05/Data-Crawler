<script type="text/javascript">
    $(function () {
        getVisitCount();
    });
    function getVisitCount() {
        $.ajax({
            url: '../ashx/ajax.ashx?type=VisitCount&rnd=' + Math.random(),
            type: 'POST',
            success: function (data) {
                var jdata = $.parseJSON(data);
                $("#spVisitCount").html(jdata);
            },
            error: function () {
            }
        });
    }
</script>

    <!--网站底部end-->


<script type="text/javascript">
    var tmptxt = $("#StranLink").html();
    var startTime = null; //报名开始时间
    var endTime = null; //报名截止时间
    var actEndTime = null; //活动结束时间
    var resNumber = null; //报名人数
    var number = null; //场馆容纳人数
    var otherNumber = 0; //弹性人数
    var reserveSeatNum = 0; //预留座位
    var yearTemp = ""; //年份
    var dateTemp = ""; //时间
    var datatype = ""; //类型

    $(function () {

        switch (No) {
            case "HD": //活动
                datatype = "Activity";
                break;
            case "JZ": //讲座
                datatype = "Lecture";
                break;
            default:
                datatype = "Activity"; //活动
                break;
        }

        InitActivityList();
    });

    //获取列表数据
    function InitActivityList() {
        $.ajax({
            url: '../ashx/ajax.ashx?type=' + datatype + '&year=' + yearTemp + '&page=1&PageSize=4&fldSort=StartTime&Sort=1&rnd=' + Math.random(),
            type: 'POST',
            success: function (data) {
                var jdata = $.parseJSON(data);
                GetActHtml(jdata);

                if (jdata.PageCount > 1) {
                    $("#pager").show();

                    $("select").change(function () {
                        $("#pager").addClass($("select").find("option:selected").text());
                    });

                    //加载分页控件
                    $("#pager").pager({
                        pagenumber: jdata.CurrentPage,
                        pagecount: jdata.PageCount,
                        buttonClickCallback: PageClick
                    });
                } else {
                    $("#pager").hide();
                }
            },
            error: function () {
            }
        });
    }

    //分页方法
    PageClick = function (pageclickednumber) {
        $.ajax({
            url: '../ashx/ajax.ashx?type=' + datatype + '&year=' + yearTemp + '&page=' + pageclickednumber + '&PageSize=4&fldSort=StartTime&Sort=1&rnd=' + Math.random(),
            type: 'POST',
            success: function (data) {
                var jdata = $.parseJSON(data);
                GetActHtml(jdata);

                //加载分页控件
                $("#pager").pager({
                    pagenumber: pageclickednumber,
                    pagecount: jdata.PageCount,
                    buttonClickCallback: PageClick
                });
            },
            error: function () {
            }
        });
    };

    //生成教育活动html
    function GetActHtml(jdata) {
        var strHtml = '';

        for (var i = 0; i < jdata.Rows.length; i++) {
            reserveSeatNum = jdata.Rows[i].ReserveSeatNum == null ? "0" : jdata.Rows[i].ReserveSeatNum;
            startTime = jdata.Rows[i].MAppointStartTime;
            endTime = jdata.Rows[i].MAppointEndTime;
            actEndTime = jdata.Rows[i].EndTime;
            resNumber = (jdata.Rows[i].ReservationCount == null ? "" : jdata.Rows[i].ReservationCount) == "" ? 0 : jdata.Rows[i].ReservationCount;
            number = (jdata.Rows[i].Number == null ? "" : jdata.Rows[i].Number) == "" ? 0 : jdata.Rows[i].Number;
            otherNumber = (jdata.Rows[i].ElasticityNumber == null ? "" : jdata.Rows[i].ElasticityNumber) == "" ? 0 : jdata.Rows[i].ElasticityNumber;
            var tempClass = 'actnow'; //样式

            if ((i + 1) % 2 == 0) {
                strHtml += '<li style="margin-right: 0;">';
            } else {
                strHtml += '<li>';
            }

            strHtml += '<div class="actlisttop">';
            strHtml += '<img src="' + jdata.ImagesThumbCoverUrl + jdata.Rows[i].WebCoverImg + '" />';
            strHtml += '<div class="actlisttopbg">';
            strHtml += '</div>';
            strHtml += '<h3>' + jdata.Rows[i].Name + '</h3>';
            strHtml += '<div class="actlisttop_link">';

            //需要预约的活动才判断预约状态
            if (jdata.Rows[i].IsAppoint == 0 || jdata.Rows[i].IsAppoint == null) {
                if (comparisonDate() == 0) {
                    if (parseInt(resNumber) + parseInt(reserveSeatNum) >= parseInt(number) + parseInt(otherNumber)) {
                        tempClass = "actused";
                        strHtml += '<i></i><a>报名人数已满</a>';
                    } else {
                        if (resNumber == number) {
                            if (otherNumber == 0) {
                                tempClass = "actused";
                                strHtml += '<i></i><a>报名人数已满</a>';
                            } else {
                                strHtml += '<i class="icon1"></i><a href="/Views/ActivityDetail.aspx?PNo=' + PNo + '&No=' + No + '&Guid=' + jdata.Rows[i].Guid + '&Type=Detail" class="appoint">+ 预约</a> ';
                            }
                        } else {
                            strHtml += '<i class="icon1"></i><a href="/Views/ActivityDetail.aspx?PNo=' + PNo + '&No=' + No + '&Guid=' + jdata.Rows[i].Guid + '&Type=Detail" class="appoint">+ 预约</a> ';
                        }
                    }
                } else if (comparisonDate() == 1) {
                    tempClass = "actused";
                    strHtml += '<i></i><a>报名未开始</a>';
                } else if (comparisonDate() == 2) {
                    tempClass = "actused";
                    strHtml += '<i></i><a>报名已结束</a>';
                } else {
                    tempClass = "actused";
                    strHtml += '<i></i><a>活动已结束</a>';
                }
            } else {
                tempClass = "none"; //标记为无需预约，不显示预约人数
                if (comparisonDate() == 3) {
                    strHtml += '<i></i><a>活动已结束</a>';
                } else {
                    strHtml += '<i></i><a>无需预约</a>';
                }
            }

            strHtml += '<a href="/Views/ActivityDetail.aspx?PNo=' + PNo + '&No=' + No + '&Guid=' + jdata.Rows[i].Guid + '&Type=Detail" class="detail">查看详情</a></div>';
            strHtml += '</div>';
            strHtml += '<div class="actlistintro">';
            strHtml += '<p><strong>时间：</strong>' + showDateTime(jdata.Rows[i].StartTime, jdata.Rows[i].EndTime) + '</p>';

            //需要预约的活动才显示报名时间
            if (jdata.Rows[i].IsAppoint == 0 || jdata.Rows[i].IsAppoint == null) {
                strHtml += '<p><strong>报名时间：</strong>' + jdata.Rows[i].MAppointStartTime + ' - ' + jdata.Rows[i].MAppointEndTime + '</p>';
            }
            strHtml += '<p><strong>地点：</strong>' + jdata.Rows[i].PlaceName + '</p>';

            if (tempClass != "none") {
                strHtml += '<p><strong>简介：</strong>' + getSubString(ReplaceHtmlLael(ReplaceHtml(jdata.Rows[i].Describe)), 30) + '</p>';
                strHtml += '<p class="' + tempClass + '">已预约' + jdata.Rows[i].ReservationCount + '人/总人数' + jdata.Rows[i].Number + '人</p>';
            } else {
                strHtml += '<p><strong>简介：</strong>' + getSubString(ReplaceHtmlLael(ReplaceHtml(jdata.Rows[i].Describe)), 80) + '</p>';
            }
            strHtml += '</div>';
            strHtml += '</li>';
        }
        $("#ulActivityList").html(strHtml);
        //简繁体转换
        StranBody(tmptxt);
    }

    //比较日期
    //返回值：0-表示可以预约；1-表示预约未开始；2-表示预约已结束；3-活动已结束
    function comparisonDate() {
        var result = 0;
        if (comptime('', actEndTime)) {
            result = 3;
        } else if (comptime('', endTime)) {
            result = 2;
        } else if (!comptime('', startTime)) {
            result = 1;
        }
        return result;
    }
    
</script>
</body></html>