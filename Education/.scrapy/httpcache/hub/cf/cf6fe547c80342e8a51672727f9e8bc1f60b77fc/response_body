<script type="text/javascript">
    $(".headtitlelist1 li").hover(function () {
        $($(".headtitlelist1 li").find(".headtitlelist2")).hide();
        $($(this).find(".headtitlelist2")).show();
    }, function () {
        $($(this).find(".headtitlelist2")).hide();
        $($(".headtitlelist1 li").find(".headtitlelistshow")).show();
    });

    //主菜单选中样式
    //    if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|opera mini|opera mobile|appleWebkit.*mobile|mobile/i.test(navigator.userAgent)) {
    //        $("head").append("<link>");
    //        css = $("head").children(":last");
    //        css.attr({
    //            rel: "stylesheet",
    //            type: "text/css",
    //            href: "../Styles/wapcommon.css"
    //        });
    //    }

    $(document).ready(function () {
        ChangeNavStyle();

        function ChangeNavStyle() {

            var nav = $(".headtitle");
            if (nav == null) {
                return;
            } else {
                var links = $(".headtitle ul.headtitlelist1 li");
                var currenturl = window.location.href;
                var currentPNo = getQueryStrings("PNo", currenturl);
                var currentNo = getQueryStrings("No", currenturl);
                var index = false;
                if (currentPNo == null) { //不属于8大栏目
                    return;
                } else {
                    for (var i = 0; i < links.length; i++) {
                        if (index == false) {
                            var lilen = $(links[i]).find("a");
                            for (var k = 0; k < lilen.length; k++) {
                                var linkurl = $(lilen[k]).attr("href");
                                if (linkurl == undefined) {
                                    return;
                                }
                                var linkPNo = getQueryStrings("PNo", linkurl);
                                if (linkPNo == currentPNo) {
                                    var last = i;
                                    index = true;

                                    $(links[last]).addClass("chose");
                                    $($(links[last]).find(".headtitlelist2")).show();
                                    if (currentNo == currentPNo) {
                                        $($(links[last]).find(".headtitlelist2")).addClass("headtitlelistshow");
                                    } else {
                                        var headtitlelist2_a = $($(links[last]).find(".headtitlelist2 a"));
                                        for (var j = 0; j < headtitlelist2_a.length; j++) {
                                            var headtitlelist2_ahref = $(headtitlelist2_a[j]).attr("href");
                                            var linkNo = getQueryStrings("No", headtitlelist2_ahref);
                                            if (linkNo == currentNo) {
                                                $(headtitlelist2_a[j]).addClass("chose");
                                                $($(links[last]).find(".headtitlelist2")).addClass("headtitlelistshow");
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    });
</script>
<script>
    ///简繁体转换

    //此转换代码转贴自网络特此说明
    var Default_isFT = 0        //默认是否繁体，0-简体，1-繁体
    var StranIt_Delay = 50      //翻译延时毫秒（设这个的目的是让网页先流畅的显现出来）
    //－－－－－－－代码开始，以下别改－－－－－－－
    //转换文本
    function StranText(txt, toFT, chgTxt) {
        if (txt == "" || txt == null) return "";
        toFT = toFT == null ? BodyIsFt : toFT;
        if (chgTxt) txt = txt.replace((toFT ? "简" : "繁"), (toFT ? "繁" : "简"));
        if (toFT) {
            return Traditionalized(txt);
        }
        else {
            return Simplized(txt);
        }
    }
    //转换对象，使用递归，逐层剥到文本
    function StranBody(fobj) {
        if (typeof (fobj) == "object") {
            var obj = fobj.childNodes;
        }
        else if (fobj) {
            //        if (fobj.indexOf("简") > 0) {
            //            BodyIsFt = 1
            //        }
            //        else {
            //            BodyIsFt = 0
            //        }
            //        setCookie(JF_cn, BodyIsFt, 7)
            //        var obj = document.body.childNodes
            var tmptxt = StranLink_Obj.innerHTML.toString();
            if (tmptxt.indexOf("简") >= 0) {
                BodyIsFt = 1;
            }
            else {
                BodyIsFt = 0;
            }
            var obj = document.body.childNodes;
        }
        else {
            var tmptxt = StranLink_Obj.innerHTML.toString();
            if (tmptxt.indexOf("简") < 0) {
                BodyIsFt = 1;
                StranLink_Obj.innerHTML = StranText(tmptxt, 0, 1);
                StranLink.title = StranText(StranLink.title, 0, 1);
            }
            else {
                BodyIsFt = 0;
                StranLink_Obj.innerHTML = StranText(tmptxt, 1, 1);
                StranLink.title = StranText(StranLink.title, 1, 1);
            }
            setCookie(JF_cn, BodyIsFt, 7)
            var obj = document.body.childNodes;
        }
        for (var i = 0; i < obj.length; i++) {
            var OO = obj.item(i);
            if ("||BR|HR|TEXTAREA|".indexOf("|" + OO.tagName + "|") > 0 || OO == StranLink_Obj) continue;
            if (OO.title != "" && OO.title != null) OO.title = StranText(OO.title);
            if (OO.alt != "" && OO.alt != null) OO.alt = StranText(OO.alt);
            if (OO.tagName == "INPUT" && OO.value != "" && OO.type != "text" && OO.type != "hidden") OO.value = StranText(OO.value);
            if (OO.nodeType == 3) {
                OO.data = StranText(OO.data);
            } else StranBody(OO);
        }
    } //欢迎来到站长特效网，我们的网址是www.zzjs.net，很好记，zz站长，js就是js特效，本站收集大量高质量js代码，还有许多广告代码下载。
    function JTPYStr() {
