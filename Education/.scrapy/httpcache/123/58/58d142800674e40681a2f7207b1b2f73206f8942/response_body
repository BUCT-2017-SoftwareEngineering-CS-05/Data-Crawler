        //初始化
        var def = "1";
        function mover(object) {
            //主菜单 选中
            var mm = document.getElementById("m_" + object);
            mm.className = "m_li_a";
            //初始主菜单 其他的不选中
            for (var i = 1; i <= 9; i++) {
                if (i != object) {
                    var mdef = document.getElementById("m_" + i);
                    mdef.className = "m_li";
                }
            }
        }
    </script>
