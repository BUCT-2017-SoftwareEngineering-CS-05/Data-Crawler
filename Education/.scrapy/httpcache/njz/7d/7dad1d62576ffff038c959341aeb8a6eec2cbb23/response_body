<script type="text/javascript">
        //绑定页面滚动事件
        $(window).bind('scroll',function(){
            var len=$(this).scrollTop()
            if(len>=200){
                $('#fhdb').show();
            }else{
                $('#fhdb').hide();
            }
        })
		$("#fhdb").smoothScroll();
