			var ownId = '27070';
			var parentId = '27068'
			$.column({
				upslope: 2,
				filename: 'tree_3601.js',
				callback: function (data) {
					if (data.result) {
						var rows = data.rows;
						var tempHTML = '';
						for (var i = 0; i < rows.length; i++) {
							var chnls = rows[i].children;
							if (chnls.length > 0) {
								var parClass = parentId == rows[i].channelid ? ' on' : '',
									isShow = parentId == rows[i].channelid ? ' style="display:block"' : '';
								tempHTML += '<li><a class="parent' + parClass + '" href="' + rows[i].url + '">' + rows[i].chnldesc + '</a><ul' + isShow + '>'
								for (var n = 0; n < chnls.length; n++) {
									var ownClass = ownId == chnls[n].channelid ? ' style="color:#f33734"' : '';
									var tempChnls=chnls[n].chnldesc
									if(tempChnls.length>12){
										tempChnls = tempChnls.slice(0,12)+"..."
									}
									tempHTML += '<li><a title='+ chnls[n].chnldesc +' '+ ownClass + ' href="' + chnls[n].url + '">' + tempChnls + '</a></li>'
								}
								tempHTML += '</ul></li>';
							} else {
								var ownClass = ownId == rows[i].channelid ? 'class=on' : '';
								tempHTML += '<li><a '+ ownClass + ' href="' + rows[i].url + '">' + rows[i].chnldesc + '</a></li>'
							}
						}
						$("#tree").html(tempHTML);
					}
				}
			})
		</script>
		<script>
			$(function () {
				if ($('.news_r_img li').length > 0) {
					var colCount   // 列数
					var colHeightArray = []  //列高度的空数组
					var imgWidth = $('.news_r_img ul li').outerWidth(true)
					var max_minHeight = 0
					//outerWidth(true) 表示元素所占用的全部的大小 左右边距等
					colCount = Math.floor($('.news_r_img').width() / imgWidth)
					for (var i = 0; i < colCount; i++) {
						colHeightArray[i] = 0
					} // 数组初始化为0 
					$('.news_r_img li').each(function () {
						var minValue = colHeightArray[0]
						var minIndex = 0
						for (var i = 0; i < colCount; i++) {
							if (colHeightArray[i] < minValue) {
								minValue = colHeightArray[i]
								minIndex = i
							}
						}
						$(this).css({
							left: minIndex * imgWidth,
							top: minValue
						})
						var minHeight = Number(minValue) + Number($(this).css("height").split("px")[0]);
						if(minHeight>max_minHeight){
							max_minHeight = minHeight
						}
						colHeightArray[minIndex] += $(this).outerHeight(true)
					})
					$(".news_r_img").css("min-height",max_minHeight + "px")
					var re;
					$(window).on('resize', function () {
						max_minHeight = 0
						clearTimeout(re)
						re = setTimeout(function () {
							console.log("resize")
							colHeightArray = []
							colCount = Math.floor($('.news_r_img').width() / imgWidth)
							for (var i = 0; i < colCount; i++) {
								colHeightArray[i] = 0
							} // 数组初始化为0 
							$('.news_r_img li').each(function () {
								var minValue = colHeightArray[0]
								var minIndex = 0
								for (var i = 0; i < colCount; i++) {
									if (colHeightArray[i] < minValue) {
										minValue = colHeightArray[i]
										minIndex = i
									}
								}
								$(this).css({
									left: minIndex * imgWidth,
									top: minValue
								})
								var minHeight = Number(minValue) + Number($(this).css("height").split("px")[0]);
								if(minHeight>max_minHeight){
									max_minHeight = minHeight
								}
								colHeightArray[minIndex] += $(this).outerHeight(true)
							})
							$(".news_r_img").css("min-height",max_minHeight + "px")
						}, 200)
					})
				}
			})
		</script>
	</body>
</html>