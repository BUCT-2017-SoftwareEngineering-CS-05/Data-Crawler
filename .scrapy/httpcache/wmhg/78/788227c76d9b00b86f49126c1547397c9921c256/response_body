<script type="text/javascript">
$(function() {
    window.ISLONGIN = false;
    $.get("/Site/Index/get_user_info.html" + '?' + Math.random(), function(data) {
        if (data.status == 1) {
            $('#logins').html('<a class="tools-item icon-user" href="/member/index.html"></a><a class="tools-item icon-logout" href="/passport/logout.html"></a>');
            window.ISLONGIN = true;
        } else {
            $('#logins').html('<a class="tools-item icon-user nolog" href="/passport/login.html"></a>');
        }
    })
});
</script>
