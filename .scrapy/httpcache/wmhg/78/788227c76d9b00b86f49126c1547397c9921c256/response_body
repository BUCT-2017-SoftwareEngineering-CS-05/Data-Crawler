<script> 
$(function(){
    // if($(window).width() > 1680) {
    //  $('.guide .section5 .title').addClass('show');
    //  $('.guide .section5 .t_btn').hide();
    // }
    $(document).ready(function() {
    
        $('.imageFullScreen').each(function() {
            $(this).smartZoom({'containerClass':'zoomableContainer'});        
        });

        $('.zoomInButton, .zoomOutButton').bind("click", zoomButtonClickHandler);

        function zoomButtonClickHandler(e){
            var zoomImg = $(this).parents('.tab-item').find('.imageFullScreen')
            var scaleToAdd = 0.2;
            if($(this).hasClass('zoomOutButton')) {
                scaleToAdd = -scaleToAdd;
            }
            zoomImg.smartZoom('zoom', scaleToAdd);
        };      

    });


    $('.section5 .t_btn').on('click', function(){
        if(!$('.section5 .title').hasClass('show')){
            $('.section5 .title').addClass('show');
            $('.guide .section5 .tab-mod').addClass('mask-show');
        }else{
            $('.section5 .title').removeClass('show');
            $('.guide .section5 .tab-mod').removeClass('mask-show');
        }
    });

    $('.guide .section5 .mask1').on('click',function() {
        $('.section5 .title').removeClass('show');
        $('.guide .section5 .tab-mod').removeClass('mask-show');
    });
    
    var as = audiojs.createAll();
    
    $('.rule').mCustomScrollbar();
    
    $('.section5 .voice-box .adlistbtn').on('click', function(){
        $('.section5 .pop-box').fadeIn();
    });
    
    $('.section5 .pop-close').on('click', function(){
        $('.section5 .pop-box').fadeOut();
    });
    
    $('.section5 .list-box').mCustomScrollbar({
        scrollbarPosition: 'outside'
    });
    
    $('.section5 .list-item').on('click', function(){
        var $this = $(this);
        var src = $this.data('src');
        var anow = as[0];
        
        if(!$this.hasClass('cur')){
            $this.addClass('cur').siblings().removeClass('cur');
            anow.load(src);
            anow.play();
            $('.section5 .pop-box').fadeOut();
        }
    });
    
        
    (function Clock(){

        var hours = new Date().getHours(),
                minutes = new Date().getMinutes(),
                seconds = new Date().getSeconds();
        
        var hrotate = hours * 30 + (minutes / 2);
        var mrotate = minutes * 6;
        var srotate = seconds * 6;
        
        TweenMax.set($(".clock-box .h"), {rotation: hrotate});
        TweenMax.set($(".clock-box .m"), {rotation: mrotate}); 
        TweenMax.set($(".clock-box .s"), {rotation: srotate});

        setTimeout(Clock,1000);
     })();
        

/// 
});
</script>
