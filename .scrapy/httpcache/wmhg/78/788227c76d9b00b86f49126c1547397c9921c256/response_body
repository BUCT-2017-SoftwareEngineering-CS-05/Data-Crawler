<script type="text/javascript">
// 高亮
function navgl() {
    var category = "/guide.html";
    var now = (category != 'javascript:void(0);') ? category : window.location.pathname;
    now2 = now + window.location.hash;
    category_top_id = "2";
    var categoryURL = "/guide.html" ;
    var cateparentid = "2";
    var cateid = "1";
    var href = ''; 
    $('.nav-lv1 .nav-item.fri').each(function(index) {
        var _this = $(this);
        var _index = index;
        href = _this.find('a.fria').attr('href'); 
        if (now == '/index.html' || now == '/index' || now == '/') {
            $('.nav-link.fria:first').parents().addClass('cur');
            
            return false;
        }
        // 一级
        try {
           if (href.indexOf(now) > -1) {
                _this.addClass('cur');
            }
        } catch (e) {}
        var isShow = true;
        // 二级
        if (_this.find('.nav-lv2 div').size() > 0) {
            _this.find('.nav-lv2 a').each(function(i) {
                href = $(this).attr('href');
                if (isShow && href.indexOf(now2) > -1) {
                    _this.addClass("cur").siblings().removeClass('cur');
                    $(this).parent().addClass('cur').siblings().removeClass('cur');
                    isShow = false;
                }
            })
        }
    })
}
navgl();
</script></div>    </div>
