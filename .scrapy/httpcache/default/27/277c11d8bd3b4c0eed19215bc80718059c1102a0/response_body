<script type="text/javascript">
    $(function () {
        getVisitCount();
    });
    function getVisitCount() {
        $.ajax({
            url: '../ashx/ajax.ashx?type=VisitCount&rnd=' + Math.random(),
            type: 'POST',
            success: function (data) {
                var jdata = $.parseJSON(data);
                $("#spVisitCount").html(jdata);
            },
            error: function () {
            }
        });
    }
</script>

    <!--网站底部end-->
</body>
</html>
<script type="text/javascript">
    var tmptxt = $("#StranLink").html();
    $(function () {
        InitImgList();
    });

    //获取图片列表数据
    function InitImgList() {
        $("#ulImageList").html("");
        $.ajax({
            url: '../ashx/ajax.ashx?type=Archives&channelNo=' + No + '&page=1&PageSize=6&fldSort=PublishDate&Sort=1&rnd=' + Math.random(),
            type: 'POST',
            success: function (data) {
                var jdata = $.parseJSON(data);
                getImgHtml(jdata);
                if (jdata.PageCount > 1) {
                    $("#pagerImg").show();

                    $("select").change(function () {
                        $("#pagerImg").addClass($("select").find("option:selected").text());
                    });

                    //加载分页控件
                    $("#pagerImg").pager({
                        pagenumber: jdata.CurrentPage,
                        pagecount: jdata.PageCount,
                        buttonClickCallback: PageClickImg
                    });
                } else {
                    $("#pagerImg").hide();
                }
            },
            error: function () {
            }
        });
    }

    //分页方法
    PageClickImg = function (pageclickednumber) {
        $.ajax({
            url: '../ashx/ajax.ashx?type=Archives&channelNo=' + No + '&page=' + pageclickednumber + '&PageSize=6&fldSort=PublishDate&Sort=1&rnd=' + Math.random(),
            type: 'POST',
            success: function (data) {
                $("#ulImageList").html("");
                var jdata = $.parseJSON(data);
                getImgHtml(jdata);
              
                //加载分页控件
                $("#pagerImg").pager({
                    pagenumber: pageclickednumber,
                    pagecount: jdata.PageCount,
                    buttonClickCallback: PageClickImg
                });
            },
            error: function () {
            }
        });
    };

    //生成九宫格html
    function getImgHtml(jdata) {
        var strHtml = '<div class="blank"></div>';
        for (var i = 0; i < jdata.Rows.length; i++) {
            var url = '/Views/ArtGoodsDetail.aspx?PNo=' + (jdata.Rows[i].ParentChannelNo == null ? jdata.Rows[i].ChannelNo : jdata.Rows[i].ParentChannelNo) + '&No=' + jdata.Rows[i].ChannelNo + '&Guid=' + jdata.Rows[i].Guid + '&Type=Detail';
            if ((i + 1) % 3 == 0) {
                strHtml += '<li style="margin-right: 0;">';
            } else {
                strHtml += '<li>';
            }

            strHtml += '<a href="' + url + '" title="' + jdata.Rows[i].Title + '" class="fulldiv"></a>';
            strHtml += '<i><img src="' + (jdata.Rows[i].ThumImg == null ? "" : (jdata.ImagesThumbUrl + jdata.Rows[i].ThumImg.replace('/', '/s_'))) + '" /></i>';
            strHtml += '<p>' + jdata.Rows[i].Title + '<span>高清</span></p>';
            if (jdata.Rows[i].Href != "") {
                strHtml += '<a href="' + jdata.Rows[i].Href + '" target="_blank" class="threed"></a>';
            }
            strHtml += '</li>';
        }

        $("#ulImageList").html(strHtml);

        $("#ulImageList li i img").each(function () {
            if ($.browser.msie && ($.browser.version == "7.0" || $.browser.version == "8.0" || $.browser.version == "6.0")) {
                GetImg(this, '350', '300');
            } else {
                GetImgFirst(this, '350', '300');
            }
        });
        //简繁体转换
        StranBody(tmptxt);
    }
</script>
