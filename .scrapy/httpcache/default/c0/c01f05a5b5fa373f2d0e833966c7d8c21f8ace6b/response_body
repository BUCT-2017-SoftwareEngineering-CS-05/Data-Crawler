              <td height="20"><style>
.pageDiv{font-size:13px;}
.pageDiv a{font-size:13px;text-decoration:none;}
</style>
<div class="pageDiv">
<script type="text/javascript">
var pageUrl=new Array();
var pageSize=1000;
pageUrl[1]="/channels/ch00079/index.shtml";
var totalCount = 32;
var perSize = 8;
var startPage=1;
var endPage=4;
var curPage =1;
var logicTotalPage = 4;

var re=/^\+?[1-9][0-9]*$/;

var nowStartPage=1;
if(request('page'))
{
  nowStartPage=request('page');
} 
document.write('共 '+totalCount+' 条，每页 '+perSize+' 条，页次<span id="curPageShow"></span> / '+logicTotalPage+'页，');
</script>
<a href="javascript:changePage(1);">首页</a> 
<a href="javascript:changePage(startPage+parseInt(curPage)-2);" id="prevP">上页 </a>
<a href="javascript:changePage(startPage+parseInt(curPage));" id="nextP">下页 </a>
<a href="javascript:changePage(logicTotalPage);">尾页</a> 
跳转到第<input id="pageSelect" type="text" value="1"   style="width:35px;"/>页 
<script type="text/javascript">
var valCurrentPage = $("#pageSelect");	 
chgpage(nowStartPage);
function chgpage(pageNum){  
	valCurrentPage.val(pageNum);
	$('#prevP').show();	$('#nextP').show();
	if(pageNum < 1 || pageNum > logicTotalPage) return;
	
	if(pageNum == 1) $('#prevP').hide();
	if(pageNum == logicTotalPage) $('#nextP').hide();
	curPage = pageNum-startPage+1 ;
 
	$('.pagedContent').hide();
	$('.pagedContent').slice((curPage-1)*perSize,curPage*perSize).show();
	$('#curPageShow').text(pageNum); 	
}
valCurrentPage.keydown(
		function(event){
			var targetPage = parseInt($(this).val());
			if(event.keyCode==13 ){				  
              if(targetPage<1){
			 	  	alert("页数不能小于1");
			 	  	return false;
			 	  }			
			   if(targetPage>logicTotalPage){
			 	  	alert("页数不能大于"+logicTotalPage);
			 	  	return false;
			 	  }				 
			 	  changePage(targetPage);
			}
		}
	);
	
 function  changePage(changePage)
 {   
 	   if(changePage<startPage||changePage>endPage)
    {         
      var rPage=Math.ceil(changePage*perSize/pageSize);     
      
      if(rPage==0){
      	 rPage=1;
      }       
       var rUrl=addParam(pageUrl[rPage],"page",changePage);
      window.location.href=rUrl;
      return ;
    }
    chgpage(changePage); 
    
 }			
</script>
</div>

