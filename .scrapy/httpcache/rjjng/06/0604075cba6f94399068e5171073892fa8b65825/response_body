<script language="JavaScript" type="text/javascript">

function Show_Menu(id_num,num){

for(var i=0;i<2;i++){document.getElementById("content_"+id_num+i).style.display="none";}

for(var i=0;i<2;i++){document.getElementById("qiehuan_"+id_num+i).className="";}

